{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-unused-vars": "off",
        "comma-dangle": "error",
        "no-console": ["warn", { "allow": ["warn", "error"] }],
        "max-len": "off",
        "import/extensions": "off",
        "prefer-destructuring": "off",
        "arrow-body-style": "off",
        "linebreak-style": "off",
        "no-useless-return": "off",
        "consistent-return": "off"
    }
}
